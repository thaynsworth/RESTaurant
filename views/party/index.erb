<h2>All Parties</h2>

<ul>
	<% @party.each do |party| %>
		<li>
			<a href="/parties/<%= party.id %>">
 			Name: <%= party.name %> |||
			Table Number: <%= party.table_num %> |||
			Party Size: <%= party.party_size %> |||
			Paid: <%= party.paid %></a>
		</li>
	<% end %>
</ul>

<a href="/">Home</a>
<a href="/parties/new">Add Party</a>
